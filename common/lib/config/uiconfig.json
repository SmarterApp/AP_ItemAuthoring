{
    "toolbar": {
        "ribbons": {
            "options": {
                "sortabletabs": "true"
            },
            "elements": {
                "Edit": {
                    "Clipboard": {
                        "subpanels": [
                            {
                                "size": "largePanel",
                                "displaytext": "true",
                                "actions": [
                                    "paste"
                                ]
                            },
                            {
                                "size": "tinyPanel",
                                "actions": [
                                    "cut",
                                    "copy",
                                    "paste-special-dialog"
                                ]
                            }
                        ]
                    },
                    "Editing" : {
                        "options": {
                            "size": "mediumPanel"
                        },
                        "actions": [
                            "undo",
                            "redo",
                            "select-all",
                            "find-replace-dialog",
                            "insert-special-character-dialog",
                            "insert-image",
                            "toggle-source-view"
                        ]
                    },
                    "Insert" : {
                        "options": {
                            "size": "tinyPanel"
                        },
                        "actions": [
                            "insert-my-nbsp",
                            "insert-indent",
                            "insert-nbsp-22",
                            "insert-rs-quote",
                            "insert-ld-quote",
                            "insert-rd-quote",
                            "insert-endash",
                            "insert-ellipsis"
                        ]
                    },
                    "Math" : {
                        "options": {
                            "size": "tinyPanel"
                        },
                        "actions": [
                            "insert-negative",
                            "insert-times",
                            "insert-divide",
                            "insert-degree",
                            "insert-pi",
                            "insert-le",
                            "insert-ge",
                            "insert-pm"
                        ]
                    },
                    "Table" : {
                        "options": {
                            "size": "largePanel"
                        },
                        "actions": [
                            "insert-table-dialog"
                        ]
                    }
                },
                "Format": {  
                    "Formatting": {
                        "options": {
                            "size": "smallPanel"
                        },
                        "actions": [
                            "bold",
                            "italic",
                            "qti-underline",
                            "qti-strikethrough",
                            "script-sub",
                            "script-super",
                            "set-no-wrap",
                            "color-dialog",
                            "backgroundcolor-dialog",
                            "clear-formatting"
                        ]
                    },
                    "Paragraph": {
                        "options": {
                            "size": "smallPanel"
                        },
                        "actions": [
                            "qti-align-left",
                            "qti-align-center",
                            "qti-align-right",
                            "qti-align-justify",
                            "list-ordered",
                            "list-unordered",
                            "qti-decrease-indent",
                            "qti-increase-indent"
                        ]
                    },
                    "Font": {
                        "subpanels": [
                            {
                                "size": "dropdownPanel",
                                "comboactions": {
                                    "font-family": {
                                        "options": {
                                            "comboWidth": "100",
                                            "dropdownWidth": "100"
                                        },
                                        "dropdownactions": {
                                            "qti-font-family-default" : "",
                                            "qti-font-family-arial": "",
                                            "qti-font-family-times": "",
                                            "qti-font-family-courier": ""
                                        }
                                    },
                                    "font-size": {
                                        "options": {
                                            "comboWidth": "100",
                                            "dropdownWidth": "100"
                                        },
                                        "dropdownactions": {
                                            "qti-font-size-default": "",
                                            "qti-font-size-8": "",
                                            "qti-font-size-9": "",
                                            "qti-font-size-10": "",
                                            "qti-font-size-11": "",
                                            "qti-font-size-12": "",
                                            "qti-font-size-14": "",
                                            "qti-font-size-16": "",
                                            "qti-font-size-18": "",
                                            "qti-font-size-20": "",
                                            "qti-font-size-22": "",
                                            "qti-font-size-24": "",
                                            "qti-font-size-26": "",
                                        }
                                    }
                                }
                            }
                        ]
                    }
                },
                "Proof": {
                    "L_PROOFING_PANE": {
                        "subpanels": [
                            {
                                "size": "largePanel",
                                "displaytext": "true",
                                "actions": [
                                    "spellcheck-dialog"
                                ]
                            },
                            {
                                "size": "mediumPanel",
                                "actions": [
                                    "toggle-auto-spellcheck",
                                    "thesaurus-dialog",
                                    "auto-correct-dialog",
                                    "change-language-dialog"
                                ]
                            }
                        ]
                    },
                    "L_ANNOTATION_PANE": {
                        "subpanels": [
                            {
                                "size": "largePanel",
                                "displaytext": "true",
                                "actions": [
                                    "insert-annotation-dialog"
                                ]
                            },
                            {
                                "size": "mediumPanel",
                                "actions": [
                                    "edit-annotation-dialog",
                                    "remove-annotation"
                                ]
                            }
                        ]
                    },
                    "L_VIEW_PANE": {
                        "options": {
                            "size": "mediumPanel"
                        },
                        "actions": [
                            "show-annotations-bubbles",
                            "show-annotations-inline"
                        ]
                    }
                },
                "Track" : {
                    "Compare": {
                        "subpanels": [
                            {
                                "size": "mediumPanel",
                                "actions": [
                                    "compare-documents",
                                    "end-comparison-mode"
                                ]
                            },
                            {
                                "size": "mediumPanel",
                                "actions": [
                                    "accept-diff",
                                    "reject-diff",
                                    "accept-all-diff",
                                    "reject-all-diff"
                                ]
                            }
                        ]
                    },
                    "View": {
                        "subpanels": [
                            {
                                "size": "mediumPanel",
                                "actions": [
                                    "show-changes-inline-diff",
                                    "show-changes-insertions-diff",
                                    "show-changes-deletions-diff"
                                ]
                            }
                        ] 
                    }
                }
            }
        }
    },
    "contextmenu": {
        "linguisticsgroup": {
            "actions": [
                "thesaurus-context",
                "spellcheck-context"
            ]
        },
        "clipboardgroup": {
            "actions": [
                "cut",
                "copy",
                "paste"
            ]
        },
        "selectgroup": {
            "actions": [
                "select-all"
            ]
        },
        "formattinggroup": {
            "submenus": {
                "fontfamilymenu": {
                    "name": "L_FONT_FAMILY",
                    "fontfamilygroup": {
                        "actions": [
                            "qti-font-family-default",
                            "qti-font-family-arial",
                            "qti-font-family-times",
                            "qti-font-family-courier"
                        ]
                    }
                },
                "fontsizemenu": {
                    "name": "L_FONT_SIZE",
                    "fontsizegroup": {
                        "actions": [
                            "qti-font-size-default",
                            "qti-font-size-8",
                            "qti-font-size-9",
                            "qti-font-size-10",
                            "qti-font-size-11",
                            "qti-font-size-12",
                            "qti-font-size-14",
                            "qti-font-size-16",
                            "qti-font-size-18",
                            "qti-font-size-20",
                            "qti-font-size-22",
                            "qti-font-size-24",
                            "qti-font-size-26",
                            "qti-font-size-28",
                            "qti-font-size-36",
                            "qti-font-size-48",
                            "qti-font-size-72"
                        ]
                    }           
                },
                "alignmenu": {
                    "name": "L_ALIGN",
                    "aligngroup": {
                        "actions": [
                            "qti-align-left",
                            "qti-align-center",
                            "qti-align-right",
                            "qti-align-justify"
                        ]
                    }
                },
                "stylemenu": {
                    "name": "L_CONTEXT_STYLE",
                    "stylegroup": {
                        "actions": [
                            "bold",
                            "italic",
                            "qti-underline",
                            "qti-strikethrough"
                        ]
                    },
                    "scriptgroup": {
                        "actions": [
                          "script-super",
                          "script-sub"
                        ]
                    }
                }
            }
        },
        "tablegroup": {
            "submenus": {
                "tablemenu": {
                    "name": "L_TABLE_TAB",
                    "tableinsertgroup": {
                        "actions": [
                            "insert-table-dialog"
                        ]
                    },
                    "tablesubgroup": {
                        "actions": [
                            "delete-table",
                            "auto-fit-table-content"
                        ]
                    },
                    "tablecaptiongroup": {
                        "actions": [
                            "insert-table-caption",
                            "delete-table-caption"
                        ]
                    },
                    "tableinsertparagraphgroup": {
                        "actions": [
                            "insert-paragraph-before-table",
                            "insert-paragraph-after-table"
                        ]
                    },
                    "tablepropertiesgroup": {
                        "actions": [
                            "table-properties-dialog"
                        ]
                    },
                    "cellrowcolumnpropertiesgroup": {
                        "submenus": {
                            "columnpropertiesmenu": {
                                "name": "L_COLUMN_PANE",
                                "columngroup": {
                                    "actions": [
                                        "column-properties-dialog",
                                        "insert-column-before",
                                        "insert-column-after",
                                        "delete-column"
                                    ]
                                }
                            },
                            "rowpropertiesmenu": {
                                "name": "L_ROW_PANE",
                                "rowgroup": {
                                    "actions": [
                                        "row-properties-dialog",
                                        "insert-row-before",
                                        "insert-row-after",
                                        "delete-row"
                                    ]
                                }
                            },
                            "cellpropertiesmenu": {
                                "name": "L_CELL_PANE",
                                "cellgroup": {
                                    "actions": [
                                        "cell-properties-dialog",
                                        "merge-cells",
                                        "split-cell-dialog"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "propertiesgroup": {
            "actions": [
                "horizontal-line-properties-dialog",
                "object-properties-dialog",
                "image-properties-dialog",
                "list-properties-dialog",
                "hyperlink-properties-dialog",
                "remove-hyperlink"
            ]
        }
    }
}
