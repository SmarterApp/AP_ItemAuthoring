    title="APIPv1.0 Entry Profile Single MC/MR Item Test Instance"
    adaptive="false"
    timeDependent="false">
    <responseDeclaration identifier="RESPONSE" cardinality="multiple" baseType="identifier">
        <correctResponse>
            <value>option1</value>
            <value>option2</value>
            <value>option4</value>
        </correctResponse>
        <mapping lowerBound="0" upperBound="3" defaultValue="-2">
            <mapEntry mapKey="option1" mappedValue="1"/>
            <mapEntry mapKey="option2" mappedValue="1"/>
            <mapEntry mapKey="option3" mappedValue="-1"/>
            <mapEntry mapKey="option4" mappedValue="1"/>
            <mapEntry mapKey="option5" mappedValue="-1"/>
        </mapping>
    </responseDeclaration>
    <outcomeDeclaration identifier="SCORE" cardinality="single" baseType="integer" normalMaximum="1.0" normalMinimum="0">
        <defaultValue>
            <value>0</value>
        </defaultValue>
    </outcomeDeclaration>
    <itemBody id="theWholeItem">
        <p id="p1"><span id="a">Ms. Smith's class contains 24 students. </span><span id="b">Each student voted for his or her favorite color. </span><span id="c">The result of the class vote is shown </span><span id="z">in the table below.</span></p>
        <table id="table001">
            <caption id="d">Results of the Class Vote</caption>
            <tbody>
                <tr id="columnheadings">
                    <th id="th001"><span id="e">Color</span></th>
                    <th id="th002"><span id="f">Number of Students</span></th>
                </tr>
                <tr id="u">
                    <td id="td001"><span id="g">Red</span></td>
                    <td id="td002"><span id="h">12</span></td>
                </tr>
                <tr id="v">
                    <td id="td003"><span id="i">Blue</span></td>
                    <td id="td004"><span id="j">6</span></td>
                </tr>
                <tr id="w">
                    <td id="td005"><span id="k">Green</span></td>
                    <td id="td006"><span id="l">4</span></td>
                </tr>
                <tr id="x">
                    <td id="td007"><span id="m">Yellow</span></td>
                    <td id="td008"><span id="n">2</span></td>
                </tr>
            </tbody>
        </table>
        <choiceInteraction responseIdentifier="RESPONSE" shuffle="false" maxChoices="5">
            <prompt id="o">Indicate which of the following statements are accurate.</prompt>
            <simpleChoice identifier="option1" fixed="true">
                <p id="p">The majority of students voted for Red.</p>
            </simpleChoice>
            <simpleChoice identifier="option2" fixed="true">
                <p id="q">Twice as many students voted for Red a voted for Blue.</p>
            </simpleChoice>
            <simpleChoice identifier="option3" fixed="true">
                <p id="r">Two percent of students voted for Yellow.</p>
            </simpleChoice>
            <simpleChoice identifier="option4" fixed="true">
                <p id="s">Red received more votes thatn any other color.</p>
            </simpleChoice>
            <simpleChoice identifier="option5" fixed="true">
                <p id="t">Twenty-five percent of students voted for Green.</p>
            </simpleChoice>
        </choiceInteraction>
    </itemBody>
    <responseProcessing>
        <responseCondition>
            <responseIf>
                <isNull>
                    <variable identifier="RESPONSE"/>
                </isNull>
                <setOutcomeValue identifier="SCORE">
                    <baseValue baseType="integer">0</baseValue>
                </setOutcomeValue>
            </responseIf>
            <responseElse>
                <setOutcomeValue identifier="SCORE">
                    <mapResponse identifier="RESPONSE"/>
                </setOutcomeValue>
                <responseCondition>
                    <responseIf>
                        <equal toleranceMode="exact">
                            <variable identifier="SCORE"/>
                            <baseValue baseType="integer">3</baseValue>
                        </equal>
                    </responseIf>
                </responseCondition>
            </responseElse>
        </responseCondition>
    </responseProcessing>
