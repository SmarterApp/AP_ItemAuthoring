},
"Proof": {
  "L_PROOFING_PANE": {
     "subpanels": [
       {
         "size": "largePanel",
         "displaytext": "true",
         "actions": [
           "spellcheck-dialog"
         ]
       },
       {
         "size": "mediumPanel",
         "actions": [
           "toggle-auto-spellcheck",
           "thesaurus-dialog",
           "auto-correct-dialog",
           "change-language-dialog"
         ]
       }
     ]
  },
  "L_ANNOTATION_PANE": {
      "subpanels": [
        {
          "size": "largePanel",
          "displaytext": "true",
          "actions": [
             "insert-annotation-dialog"
          ]
        },
        {
          "size": "mediumPanel",
          "actions": [
            "edit-annotation-dialog",
            "remove-annotation"
          ]
        }
      ]
  },
  "L_VIEW_PANE": {
    "options": {
      "size": "mediumPanel"
    },
    "actions": [
      "show-annotations-bubbles",
      "show-annotations-inline"
    ]
  }
},
"Track Changes" : {
  "Compare": {
    "subpanels": [
        {
          "size": "mediumPanel",
          "actions": [
            "compare-documents-dialog",
            "end-comparison-mode"
          ]
        },
        {
          "size": "mediumPanel",
          "actions": [
               "accept-diff",
               "reject-diff",
               "accept-all-diff",
               "reject-all-diff",
          ]
        }
      ]
    },
  "View": {
    "subpanels": [
      {
        "size": "mediumPanel",
        "actions": [
          "show-changes-inline-diff",
          "show-changes-insertions-diff",
          "show-changes-deletions-diff",
        ]
      }
    ] 
  }
}


